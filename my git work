print("Enter 5 subject marks ")
eng=int(input(print("Enter your marks for English :")))
urdu=int(input(print("Enter your marks for Urdu :")))
math=int(input(print("Enter your marks for Math :")))
chem=int(input(print("Enter your marks for Chemistry :")))
phy=int(input(print("Enter your marks for Physics :")))
total=eng+urdu+math+chem+phy
print(total)
if total<100 and total >90:
    print("A+")
elif total <90 and total >80:
    print("A")
elif total<80 and total >70:
    print("B")
elif total<70 and total >60:
    print("C")
elif total<60 and total >50:
    print("D")
elif total<50 and total >40:
    print("E")
elif total<40 and total >33:
    print("F")
else:
    print("invalid number ") 
